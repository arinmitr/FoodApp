n        <span>Total Amount</span>\r\n        <span>{totalAmount}</span>\r\n      </div>\r\n      {isCheckout && (\r\n        <Checkout onCancel={props.onClose} onConfirm={submitOrderHandler} />\r\n      )}\r\n      {!isCheckout && (\r\n        <div className={classes.actions}>\r\n          <button className={classes['button--alt']} onClick={props.onClose}>\r\n            Close\r\n          </button>\r\n          {hasItems && (\r\n            <button className={classes.button} onClick={orderHandler}>\r\n              Order\r\n            </button>\r\n          )}\r\n        </div>\r\n      )}\r\n    </>\r\n  )\r\n  const submittingModalContent = <p>Order in progress. Please wait...</p>\r\n\r\n  const orderedModalContent = (\r\n    <>\r\n      <p>Ordered Successfully</p>\r\n      <div className={classes.actions}>\r\n        <button className={classes.button} onClick={props.onClose}>\r\n          Close\r\n        </button>\r\n      </div>\r\n    </>\r\n  )\r\n\r\n  return (\r\n    <Modal onClose={props.onClose}>\r\n      {!isSubmitting && cartModalContent}\r\n      {isSubmitting && !ordered && submittingModalContent}\r\n      {ordered && orderedModalContent}\r\n    </Modal>\r\n  )\r\n}\r\n\r\nexport default Cart\r\n","I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\Layout\\HeaderCartButton.js",[],"I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\Meals\\MealsSummary.js",[],"I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\Meals\\AvailableMeals.js",[],"I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\UI\\Modal.js",[],"I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\UI\\Card.js",[],"I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\Meals\\MealItem\\MealItem.js",[],"I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\Meals\\MealItem\\MealItemForm.js",[],"I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\UI\\Input.js",[],"I:\\Web Development\\React-Apps\\FoodApp\\src\\components\\Cart\\CartItem.js